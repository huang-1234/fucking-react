{
  "name": "web-cache",
  "version": "0.0.1",
  "description": "前端web缓存方案",
  "main": "dist/web-cache.umd.js",
  "module": "dist/web-cache.es.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/web-cache.es.js",
      "require": "./dist/web-cache.umd.js"
    },
    "./service-work": {
      "types": "./dist/types/service-work/index.d.ts",
      "import": "./dist/service-work.es.js",
      "require": "./dist/service-work.umd.js"
    }
  },
  "typesVersions": {
    "*": {
      "service-work": [
        "./dist/types/service-work/index.d.ts"
      ]
    }
  },
  "scripts": {
    "dev": "vite",
    "build:main": "node scripts/build.js main",
    "build:service-work": "node scripts/build.js service-work",
    "build": "node scripts/build.js all",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build && npm run test:coverage",
    "login": "npm login",
    "publish:all": "node scripts/publish.js all",
    "publish:all:beta": "node scripts/publish.js all:beta",
    "publish:all:alpha": "node scripts/publish.js all:alpha",
    "publish:all:inner": "node scripts/publish.js all:inner",
    "publish:main": "node scripts/publish.js main",
    "publish:main:beta": "node scripts/publish.js main:beta",
    "publish:main:alpha": "node scripts/publish.js main:alpha",
    "publish:main:inner": "node scripts/publish.js main:inner",
    "publish:service-work": "node scripts/publish.js service-work",
    "publish:service-work:beta": "node scripts/publish.js service-work:beta",
    "publish:service-work:alpha": "node scripts/publish.js service-work:alpha",
    "publish:service-work:inner": "node scripts/publish.js service-work:inner"
  },
  "keywords": [
    "cache",
    "service-worker",
    "offline",
    "pwa",
    "web-cache"
  ],
  "author": "",
  "license": "ISC",
  "resolutions": {
    "commander": "12.1.0",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "@types/react": "19.1.9",
    "@types/react-dom": "19.1.9",
    "antd": "5.26.7"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/lodash-es": "4.17.12",
    "@types/node": "22.13.10",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.1",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-react": "4.3.4",
    "axios": "1.7.9",
    "commander": "^12.0.0",
    "cross-env": "7.0.3",
    "eslint": "9.30.1",
    "lodash-es": "4.17.21",
    "prettier": "^3.6.2",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "readline": "^1.3.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "ts-node": "10.9.2",
    "typescript": "5.8.3",
    "vite": "6.0.0",
    "vite-plugin-dts": "^3.7.3",
    "vitest": "2.1.5"
  },
  "packageManager": "pnpm@10.13.1",
  "dependencies": {
    "axios": "^1.7.9",
    "chalk": "^5.3.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "file-type": "^19.0.0",
    "immer": "^10.1.1",
    "mermaid": "^11.10.1",
    "uuid": "^9.0.1"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}