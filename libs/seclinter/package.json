{
  "name": "seclinter",
  "version": "0.0.1",
  "description": "前端Web安全检测与攻防工具包",
  "main": "dist/seclinter.umd.js",
  "module": "dist/seclinter.es.js",
  "types": "dist/index.d.ts",
  "bin": {
    "seclinter": "./dist/cli/index.js"
  },
  "scripts": {
    "dev": "vite",
    "build": "node scripts/build.js",
