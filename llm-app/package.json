{
  "name": "llm-app",
  "version": "1.0.0",
  "description": "大模型应用",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "node dist/main.js",
    "start:dev": "ts-node src/main.ts",
    "start:simple": "node start-simple.js",
    "start:prod": "node dist/main.js",
    "start:serverless": "serverless offline",
    "build": "tsc -p tsconfig.build.json",
    "build:serverless": "serverless package",
    "deploy": "bash scripts/deploy.sh",
    "deploy:aws": "bash scripts/deploy.sh --provider aws",
    "deploy:aliyun": "bash scripts/deploy.sh --provider aliyun",
    "deploy:tencent": "bash scripts/deploy.sh --provider tencent",
    "deploy:assets": "bash scripts/deploy.sh --assets",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "vitest run --config vitest.e2e.config.ts",
    "lint": "eslint src --ext .ts,.tsx",
    "clean": "rimraf dist && powershell -Command \"Get-ChildItem -Path src -Recurse -Include *.js,*.d.ts | Remove-Item -Force\""
  },
  "keywords": [
    "llm",
    "server",
    "node",
    "react",
    "typescript",
    "serverless"
  ],
  "author": "",
  "license": "ISC",
  "resolutions": {
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "@types/react": "19.1.9",
    "@types/react-dom": "19.1.9",
    "antd": "5.26.7"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/websockets": "^11.1.6",
    "express": "^5.1.0",
    "isomorphic-style-loader": "^5.3.2",
    "jsdom": "^24.0.0",
    "openai": "^5.20.2",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "6.22.3",
    "rxjs": "^7.8.2",
    "serverless-http": "^3.2.0",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@codegenie/serverless-express": "4.17.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^16.3.0",
    "@types/aws-lambda": "^8.10.136",
    "@types/express": "4.17.21",
    "@types/node": "22.13.10",
    "@types/react": "19.1.9",
    "@types/react-dom": "19.1.9",
    "@types/react-helmet-async": "^1.0.3",
    "@types/socket.io": "3.0.2",
    "@types/supertest": "^6.0.2",
    "@vitejs/plugin-react": "4.3.4",
    "@vitest/coverage-v8": "2.1.5",
    "cross-env": "7.0.3",
    "css-loader": "^6.10.0",
    "eslint": "8.57.0",
    "mini-css-extract-plugin": "^2.8.1",
    "reflect-metadata": "^0.2.2",
    "rimraf": "5.0.5",
    "run-script-webpack-plugin": "^0.2.0",
    "serverless": "^3.38.0",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-offline": "^13.3.3",
    "serverless-webpack": "^5.13.0",
    "supertest": "^6.3.4",
    "ts-node": "10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "5.8.3",
    "vite": "6.0.0",
    "vitest": "2.1.5",
    "webpack-node-externals": "^3.0.0"
  },
